/* pages/data/data.wxss */
.head{
    display: flex;
}
.weui_cell{
    position: relative;
    display: flex;
    padding: 25rpx;
    -webkit-box-align: center;
    -ms-flex-align: center;
    align-items: center;
    border-bottom: 2rpx solid #dadada;
    border-top: 2rpx solid #dadada;
}
.weui_cell_hd{
  display: inline-block;
  width: 40rpx;
  margin-right: 10rpx;
}
.weui_cell_hd image{
  width: 100%;
  height: 20px;
  vertical-align: -5rpx;
}
.weui_cell_bd{
  display: inline-block;
}
.weui_cell_bd_p{
  font-size: 14px;
  color: #939393;
}
.room_name{
  align-items: center;
  font-size: 14px;
  color: #939393;
  margin-left: 485rpx;
}
.with_arrow{
    position: absolute;
    top: 18px;
    right: 15px;
    width: 15px;
    height: 15px;
    /* background-image: url(../../images/icon-arrowdown.png); */
    background-repeat: no-repeat;
    background-size: 100% 100%;
  }